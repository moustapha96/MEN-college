{% extends '././layouts/main.html.twig' %}

{% block title %}
	Client
	{{ titre }}
{% endblock %}

{#{% entete des page %}#}
{% block head %}
	{{ parent() }}
{% endblock %}

{#{% le menu %}#}
{% block menu %}
	{% include 'client/include/menu.html.twig' %}
{% endblock %}

{#{% le corps de la page %}#}
{% block content %}
	<div class="content-wrapper">
		<div class="content-header">
			<div class="container-fluid">
				<div class="row mb-2">
					<div
						class="col-sm-6 text-bold">
						{#{% titre de l'activite %}#}
						{% block title_job %}{% endblock %}
					</div>
					<div class="col-sm-6">
						<ol class="breadcrumb float-sm-right">
							<li
								class="breadcrumb-item">

								{# {% operation possible %}#}
								{% block option %}{% endblock %}

								{# <a class="btn btn-outline-primary  float-right ml-1 btn-sm"
																																																																																																																																																																																																																								                                                                href=""><i class="fa fa-reply"></i> Retour</a> #}
							</li>

						</ol>
					</div>
				</div>
			</div>
			<div class="row">
				<div class="col-md-12">
					{% for label, messages in app.flashes %}
						{% for message in messages %}
							<div class="alert alert-{{ label }}">{{ message }}</div>
						{% endfor %}
					{% endfor %}


				</div>


			</div>

			<div class="content">
				<div
					class="container-fluid">
					{# {% les donnees affichees %}#}
					{% block data %}{% endblock %}
				</div>
			</div>
		</div>
	</div>

{% endblock %}
