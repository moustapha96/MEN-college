{% extends 'client/include/main.html.twig' %}


{#{% titre de l'activite %}#}
{% block title_job %}
<h1 class="card-title"> {{ titre }} </h1>
{% endblock %}

{# {% operation possible %}#}
{% block option %}
{% for message in app.flashes('success') %}

<div class="flash-notice alert alert-success">
  {{ message }}
</div>

{% endfor %}
<a class="btn btn-outline-primary float-right ml-1 btn-sm" href="{{ path('client_rapport_liste') }}"><i
    class="fa fa-list"></i> Liste</a>
<a href=" {{ path('client_rapport_nouveau') }}" class=" btn btn-info float-right btn-sm" role="button">
  <i class="fa fa-plus" aria-hidden="true"></i> nouveau rapport </a>
{% endblock %}

{# {% les donnees affichees %}#}
{% block data %}


<div class="row">
  <div class="col-12">
    <div class="card card-margin">
      <div class="card-body">
        <div class="row search-body">
          <div class="col-lg-12">
            <div class="search-result">

              <div class="result-body">
                <h1>Edition Rapport</h1>

                {{ include('admin/rapport/_form.html.twig', {'button_label': 'Update'}) }}


                {{ include('admin/rapport/_delete_form.html.twig') }}
              </div>
            </div>
          </div>
        </div>

      </div>
    </div>
  </div>
</div>
</div>
{% endblock %}