		titre
	</h1>
{% endblock %}

{# {% operation possible %}#}
{% block option %}{% endblock %}

{# {% les donnees affichees %}#}
{% block data %}


	<div class="row">
		<div class="col-12">
			<div class="card card-margin">
				<div class="card-body">


					<div class="table-responsive">

						<table class="table">
							<tbody>
								<tr>
									<th>Id</th>
									<td>{{ parametrage.id }}</td>
								</tr>
								<tr>
									<th>Logo</th>
									<td>{{ parametrage.logo }}</td>
								</tr>
								<tr>
									<th>Name</th>
									<td>{{ parametrage.name }}</td>
								</tr>
								<tr>
									<th>Description</th>
									<td>{{ parametrage.description }}</td>
								</tr>
							</tbody>
						</table>

						<a href="{{ path('super_admin_parametrage_index') }}">back to list</a>

						<a href="{{ path('super_admin_parametrage_edit', {'id': parametrage.id}) }}">edit</a>

						{{ include('super_admin/parametrage/_delete_form.html.twig') }}


					</div>


				</div>
			</div>
		</div>

	</div>
